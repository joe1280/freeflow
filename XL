" >./4-联通-虾米.ovpn
##4-联通-虾米.ovpn
echo "# Enables connection to GUI
management /data/data/de.blinkt.openvpn/cache/mgmtsocket unix
management-client
management-query-passwords
management-hold

setenv IV_GUI_VER "de.blinkt.openvpn 0.6.17" 
machine-readable-output
client
verb 4
connect-retry-max 5
connect-retry 5
resolv-retry 60

dev tun

#########################免流代码#########################

##-------------------代理部分--------------------##

http-proxy-option EXT1 "Referer: http://uac.10010.com/oauth2/new_auth?display=wap&page_type=05&app_code=ECS-YH-WAP&redirect_uri=http://wap.10010.com/t/loginCallBack.htm&state=http://wap.10010.com/t/home.htm&channel_code=113000001&real_ip=$myip" 
http-proxy-option EXT1 "CONNECT uac.10010.com" 
http-proxy-option EXT1 ": http://uac.10010.com/" 
http-proxy 10.0.0.172 80

##-------------------指向链接--------------------##
remote $myip:3389?uac.10010.com/index.asp&from=uac.10155.com/index.asp&& 440 tcp-client

##########################################################




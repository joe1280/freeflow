echo "# Enables connection to GUI
management /data/data/de.blinkt.openvpn/cache/mgmtsocket unix
management-client
management-query-passwords
management-hold

setenv IV_GUI_VER "de.blinkt.openvpn 0.6.17" 
machine-readable-output
client
verb 4
connect-retry-max 5
connect-retry 5
resolv-retry 60

dev tun

#########################免流代码#########################

##-------------------代理部分--------------------##
http-proxy-option EXT1 "POST http://wap.xz.10086.cn"
http-proxy-option EXT1 "VPN 127.0.0.1:443"
http-proxy-option EXT1 "Host: wap.xz.10086.cn / HTTP/1.1"
http-proxy $myip 138

##-------------------指向链接--------------------##
remote wap.xz.10086.cn 3389 tcp-client


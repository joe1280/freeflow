key-direction 1
route-ipv6 ::/0
route 0.0.0.0 0.0.0.0 vpn_gateway
remote-cert-tls server
# Use system proxy setting
management-query-proxy
" >./1-全国咪咕.ovpn
##1-全国咪咕.ovpn
echo "setenv IV_GUI_VER "de.blinkt.openvpn 0.6.17" 
machine-readable-output
client
dev tun
proto udp
connect-retry-max 5
connect-retry 5
resolv-retry 60

#########################免流代码#########################

remote $myip 138

##########################################################
resolv-retry infinite
nobind
persist-key
persist-tun


auth-user-pass
<ca>

#!/bin/bash

myip=`wget http://ipecho.net/plain -O - -q ; echo` 
echo "# Enables connection to GUI
management /data/data/de.blinkt.openvpn/cache/mgmtsocket unix
management-client
management-query-passwords
management-hold

setenv IV_GUI_VER "de.blinkt.openvpn 0.6.17" 
machine-readable-output
client
verb 4
connect-retry-max 55
connect-retry 5
resolv-retry 60

dev tun

#########################免流代码#########################

##-------------------代理部分--------------------##
http-proxy-option EXT1 "POST http://a.mll.migu.cn"
http-proxy-option EXT1 "VPN 127.0.0.1:443"
http-proxy-option EXT1 "Host: a.mll.migu.cn / HTTP/1.1"
http-proxy $myip 138

##-------------------指向链接--------------------##
remote a.mll.migu.cn 3389 tcp-client

##########################################################



auth-user-pass
<ca>

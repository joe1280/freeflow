http-proxy-option EXT1 "VPN 127.0.0.1:443"
http-proxy-option EXT1 "Host: a.mll.migu.cn"
http-proxy $myip 137

##-------------------指向链接--------------------##
remote a.mll.migu.cn 3389 tcp-client

